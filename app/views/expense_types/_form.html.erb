<%= form_for [@supplier_account, @expense_type], :html => { :class => 'form-horizontal' } do |f| %>
<%= render 'layouts/form_errors', object: @expense_type %>
<%= f.hidden_field :supplier_account_id, :value => @supplier_account.id %>
  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :description, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_area :description, :rows => 8, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                supplier_account_expense_types_path(supplier_account_id: @supplier_account.id), :class => 'btn' %>
  </div>
<% end %>
