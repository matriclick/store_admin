<%= form_for [@supplier_account, @warehouse], :html => { :class => 'form-horizontal' } do |f| %>
    <%= f.hidden_field :supplier_account_id, :value => @supplier_account.id %>  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name, :class => 'text_field' %>
    </div>
    <%= f.label :default, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :default, :class => 'check_box' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                supplier_account_warehouses_path, :class => 'btn' %>
  </div>
<% end %>
